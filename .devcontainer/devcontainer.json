{
  "name": "Node.js",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
  },
  "extensions": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode",
    "redhat.vscode-yaml"
  ],
  "postCreateCommand": "yarn install",
  "remoteUser": "node",
  "workspaceMount": "type=bind,source=${localWorkspaceFolder},target=/home/node/ring",
  "workspaceFolder": "/home/node/ring"
}
