{
  "name": "ring",
  "version": "1.1.5",
  "description": "Ring",
  "author": "Hikaru Wada <watyaa@gmail.com>",
  "private": true,
  "scripts": {
    "format": "prettier --write \"./**/*.{ts,tsx}\"",
    "build:dev": "webpack",
    "build": "webpack --mode=production",
    "push": "yarn build && clasp push",
    "dev": "webpack --watch --cache & clasp push -w",
    "lint": "eslint --fix --ext .ts,.js --ignore-path .gitignore .",
    "test": "jest",
    "tsc": "echo 'We use \"yarn build\" instead of \"yarn tsc\"' && yarn build"
  },
  "dependencies": {
    "core-js": "3.20.1",
    "dayjs": "1.10.7",
    "regenerator-runtime": "0.13.9",
    "ts-is-present": "1.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/preset-env": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@types/google-apps-script": "1.0.43",
    "@types/google-apps-script-oauth2": "38.0.0",
    "@types/jest": "27.4.0",
    "@typescript-eslint/parser": "5.9.0",
    "babel-jest": "27.4.6",
    "babel-loader": "8.2.3",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-webpack-plugin": "3.1.1",
    "fork-ts-checker-webpack-plugin": "6.5.0",
    "gas-webpack-plugin": "2.1.0",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "typescript": "4.5.4",
    "webpack": "5.65.0",
    "webpack-cli": "4.9.1"
  }
}
