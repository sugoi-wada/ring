{
  "name": "Ring app",
  "dockerComposeFile": ["docker-compose.yml"],
  "service": "node",
  "settings": {
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "jest.jestCommandLine": "yarn test"
  },
  "extensions": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode",
    "redhat.vscode-yaml",
    "orta.vscode-jest"
  ],
  "postCreateCommand": "yarn install",
  "workspaceFolder": "/app"
}
